<table width="100%" class="table table-striped">
    <thead>
        <tr class="align-middle"><th>bot id</th><th>id</th><th>path</th><th>timestamp</th><th>content type</th><th>content length</th><th>headers</th><th></th><th></th></tr>
    </thead>
    <tbody>
{% for record in records %}
        <tr class="align-middle">
            <td>{{ record.botId}}</td>
            <td>{{ record.id }}</td>
            <td>{{ record.path }}</td>
            <td>{{ record.timeStamp|date('Y-m-d H:i:s') }}</td>
            <td>{{ record.contentType }}</td>
            <td>{{ record.length }}</td>
            <td>{{ record.headers|ellipsis }}</td>
            <td>
                <button class="btn btn-primary btn-sm" data-records-view-bot-id-param="{{ record.botId }}" data-records-view-id-param="{{ record.id }}" data-action="records-view#show">View</button>
            </td>
            <td>
                <button class="btn btn-secondary btn-sm" data-records-view-bot-id-param="{{ record.botId }}" data-records-view-id-param="{{ record.id }}" data-action="records-view#download">Download</button>
            </td>
        </tr>
{% endfor %}
    </tbody>
</table>

